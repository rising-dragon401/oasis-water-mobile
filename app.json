{
  "expo": {
    "name": "Oasis",
    "slug": "Oasis",
    "scheme": "oasisapp",
    "version": "0.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.liveoasis.oasis",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:live-oasis.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "oasis-expo",
          "organization": "liveoasis"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload your profile picture."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ba3ea707-c8f6-4baf-becd-c2dedfa41b76"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ba3ea707-c8f6-4baf-becd-c2dedfa41b76"
    }
  }
}
